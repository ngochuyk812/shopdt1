<body>
    <nav class="main-nav">
        <div class="left-nav">
            <h3 style="font-size: 50px;">NGOCHUY</h3>
        </div>
        <div class="right-nav">
            <span class="icon-nav"><i class="fa-solid fa-cart-shopping"></i>
                <a href="/cart"> <span style="font-size: 15px;">Giỏ hàng</span>
                </a>
            </span>
            <span class="icon-nav"><i class="fa-solid fa-user"></i>
                <a href="/profile"> <span style="font-size: 15px;">Tài khoản</span> </a>
            </span>
        </div>
    </nav>
    <section class="ftco-section" style="padding-top: 0;">
        <div class="container" style="margin-top: 0px;">
            <div class="row">
                <div class="col-md-12">
                    <div class="slider-hero">
                        <div class="featured-carousel owl-carousel">
                            <div class="item">
                                <div class="work">
                                    <div class="img d-flex align-items-center justify-content-center"
                                        style="background-image: url(banner/images/slider-1.jpg);">
                                        <div class="text text-center">
                                            <h2>Discover New Places</h2>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="work">
                                    <div class="img d-flex align-items-center justify-content-center"
                                        style="background-image: url(banner/images/slider-2.png);">
                                        <div class="text text-center">
                                            <h2>Dream Destination</h2>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="work">
                                    <div class="img d-flex align-items-center justify-content-center"
                                        style="background-image: url(banner/images/slider-31.jpg);">
                                        <div class="text text-center">
                                            <h2>Travel Exploration</h2>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="my-5 text-center">
                            <ul class="thumbnail">
                                <li class="active img"><a href="#"><img src="banner/images/thumb-1.jpg" alt="Image"
                                            class="img-fluid"></a></li>
                                <li><a href="#"><img src="banner/images/thumb-2.jpg" alt="Image" class="img-fluid"></a>
                                </li>
                                <li><a href="#"><img src="banner/images/thumb-3.jpg" alt="Image" class="img-fluid"></a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="search-sec">
        <div class="container">
            <form action="/filter" method="post" novalidate="novalidate">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-sm-12 p-0">
                                <select class="form-control search-slt sssss" name="company"
                                    id="exampleFormControlSelect1" style="font-weight: 550; font-size: 12px;"
                                    style="font-weight: 550; font-size: 12px;">
                                    <option>Hãng</option>
                                    <option>IPhone</option>
                                    <option>Samsung</option>
                                    <option>Vivo</option>
                                    <option>Xiaomi</option>
                                    <option>Readmi</option>
                                    <option>Oppo</option>
                                </select>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-12 p-0">
                                <select class="form-control search-slt" name='price' id="exampleFormControlSelect1"
                                    style="font-weight: 550; font-size: 12px;">
                                    <option>Giá</option>
                                    <option>Dưới 2 triệu</option>
                                    <option>Từ 2 - 4 triệu</option>
                                    <option>Từ 4 - 7 triệu</option>
                                    <option>Từ 7 - 13 triệu</option>
                                    <option>Từ 13 - 20 triệu</option>
                                    <option>Trên 20 triệu</option>
                                </select>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-12 p-0">
                                <select class="form-control search-slt" name='sort' id="exampleFormControlSelect1"
                                    style="font-weight: 550; font-size: 12px;">
                                    <option>Sắp xếp</option>
                                    <option>Giá tăng dần</option>
                                    <option>Giá giảm dần</option>

                                </select>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-12 p-0">
                                <button type="submit" class="btn btn-danger wrn-btn">Search</button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </section>

    <div class="main" style=" margin-top: -100px;">

        <div class="container">
            <div class="row">

                <% for(var i=0; i<list.length; i++) {%>

                    <%var a=list[i].image_link.split("/")[5]%>



                        <div class="col-md-3 col-sm-6  " style="padding: 10px;">
                            <div class="product-grid2 product-item">
                                <div class="product-image2"> <a href="#"> <img class="pic-1" src="images/<%=a%>"> <img
                                            class="pic-2" src="images/<%=a%>">
                                    </a>
                                    <ul class="social">
                                        <li><a href="/detail/<%=list[i].id  %>" data-tip="Quick View"><i
                                                    class="fa fa-eye"></i></a>
                                        </li>
                                        <form action="/add-cart" method="post">
                                            <input type="text" name="idProduct" value="<%=list[i].id%>" hidden>
                                            <li><button type="submit" data-tip="Add to Cart"
                                                    style="width: 45px; height: 45px; border-radius: 50%; border: none;"><i
                                                        class="fa fa-shopping-cart"></i></button>
                                            </li>
                                        </form>
                                    </ul>
                                </div>
                                <div class="product-content">
                                    <h3 class="title"><a href="#">
                                            <%=list[i].name%>
                                        </a></h3> <span class="price"> Price: $<%=list[i].price%>
                                    </span>
                                </div>
                            </div>
                        </div>

                        <% } %>
            </div>
        </div>
    </div>
    <hr>



</body>

